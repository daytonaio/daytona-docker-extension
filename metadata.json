{
  "icon": "daytona.svg",
  "vm": {
    "composefile": "docker-compose.yml",
    "exposes": { "socket": "daytona-docker-extension.sock" }
  },
  "ui": {
    "dashboard-tab": {
      "title": "Daytona",
      "root": "/client",
      "src": "index.html",
      "backend": {
        "socket": "daytona-docker-extension.sock"
      }
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/darwin/daytona"
          }
        ],
        "linux": [
          {
            "path": "/linux/daytona"
          }
        ],
        "windows": [
          {
            "path": "/windows/daytona.exe"
          }
        ]
      }
    ]
  }
}
